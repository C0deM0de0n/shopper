generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
  id        String    @id @default(cuid())
  createdAt DateTime  @default(now())      @map("created-at")
  updatedAt DateTime  @updatedAt           @map("updated-at")
  email     String    @unique
  password  String
}
